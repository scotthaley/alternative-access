{
  "name": "Alternative Access",
  "description": "Increase accessibility for complex movers.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "ALT-Ax"
  },
  "permissions": [
    "activeTab",
    "debugger"
  ],
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./static/js/content.js"
      ]
    }
  ]
}
